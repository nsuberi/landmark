@import "config"
@import "analysis"

.launch-dialog-title
	margin 0

.launch-dialog-subtitle
	font-style italic
	font-size 14px
	
.launch-dialog-content
	font-size 15px
	
.dijitDialog .dijitDialogCloseIcon
	outline none !important

.brMap
	background-color $mainBackground
	/* For Mobile Menu When Open */
	box-shadow -3px 0 2px #BBB
	position absolute
	bottom 0
	right 0
	left 0
	top 0
	
.map-component	
	box-sizing initial
	position absolute
	z-index 30
	
.esriControlsBR
	right 300px
	
.brMap .HomeButton .home
	background url('images/ico-home.png') no-repeat center
	border-bottom 1px solid black
	background-color white
	border-radius 0
	cursor pointer
	height 30px
	width 30px
	padding 0
	
.brMap .HomeButton .home:hover
	background-color $esriHover
	
.legend-container
	width 260px
	right 20px
	z-index 50
	bottom 0
	
.legend-title-pane
	border-bottom 1px solid $mainBorder
	padding-right 5px
	text-align right
	
.legend-title
	border-radius-top(3px)
	border 1px solid $mainBorder
	display inline-block
	background white
	line-height 30px
	border-bottom 0
	padding 0 20px
	font-size 14px
	height 30px
	
.legend-content
	border 1px solid $mainBorder
	background-color white
	border-bottom 0
	font-size 13px
	border-top 0

.legend-content:not(.active)
	overflow hidden
	
#legend
	max-height 300px
	overflow-y auto
	
#legend #legend_msg 
#legend .esriLegendServiceLabel
	display none

#legend .esriLegendService
	padding-bottom 0px

#legend > div:nth-last-child(2)
	padding-bottom 10px

.button-widget
	background-color white
	border 1px solid black
	border-radius(5px)
	height 30px
	width 30px
	right 20px
	
.basemap-button
.basemap-connector
	top 120px
	
.locator-button
	top 160px

#share-widgets
	padding 14px 15px 0

#twitterShare, #facebookShare, #moreShare, #emailShare, #googleShare, #embedShare
	display inline-block
	cursor pointer
	height 32px
	width 32px

