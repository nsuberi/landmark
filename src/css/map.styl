#embedShare
	background url(images/embed_icon.png) no-repeat #fff
	
#moreShare
	background url('data:image/svg+xml;charset=utf-8,%3Csvg%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%20width%3D%2232%22%20height%3D%2232%22%20viewBox%3D%220%200%2032%2032%22%3E%3Cpath%20fill%3D%22%23fff%22%20d%3D%22M25.07%2013.74h-6.711v-6.71h-4.469v6.71h-6.71v4.47h6.71v6.71h4.469v-6.71h6.711z%22%2F%3E%3C%2Fsvg%3E') no-repeat left
	
.share-button
.share-connector	
	top 200px

.basemap-icon
	background url('images/basemap.png')	
	
.share-icon
	background url('images/share-icon.png')
	background-position -3px -1px
	
.basemap-icon
.share-icon
	height 100%
	width 100%
	
.brMap .LocateButton .zoomLocateButton
	background-color transparent
	background-image url('images/locate.png')

.brMap .LocateButton .zoomLocateButton:hover
	background-color transparent
	background-image url('images/locate.png')
	
.brMap .LocateButton .zoomLocateButton
.basemap-icon
	background-position -2px -2px
	
.brMap .LocateButton .zoomLocateButton:hover
.basemap-icon:hover
	background-position -2px -36px
	
.share-icon:hover
	background-position -3px -36px
	
.basemap-container
	border 1px solid black
	background-color white
	border-radius(5px)
	overflow-y auto
	height 400px
	width 390px
	right 60px
	top 65px
	
.esriBasemapGalleryLabelContainer
	font-size 13px
	padding 5px 0
	
.esriBasemapGallery
	margin 10px 5px
	
.share-container
	border 1px solid black
	background-color white
	border-radius(5px)
	overflow-y auto
	min-width 50px
	height 60px
	width auto
	right 60px
	top 185px

.connector
	border-bottom 1px solid black
	border-top 1px solid black
	background-color white
	height 30px
	right 48px
	width 13px
	
.tree-widget-container
	left 20px
	top 20px
	
.tree-widget-title-pane	
	border 1px solid $mainBorder
	background-color white
	box-sizing initial
	line-height 32px
	height 32px
	
.tree-width
	width 280px
	
.tree-title-text
	border-right 1px solid $mainBorder
	display inline-block
	
.tree-title-text span
	padding 0 12px

.tree-container-toggle
	display inline-block
	padding 0 12px

.tree-content
	background-color white
	position absolute
	overflow hidden
	
.brMap .tree-content.active
	border 1px solid $mainBorder
	border-top 0

.tree-content .tester
	height 200px
	
.analysis-button
	border 1px solid $mainBorder
	border-radius(5px)
	background-color white
	text-align center
	line-height 32px
	height 32px
	right 70px
	top 20px
	
.analysis-button-label
.data-completeness-label
.data-completeness-toggle
	padding 0 10px
	
.data-completeness-container
	border 1px solid $mainBorder
	border-radius(5px)
	background-color white
	text-align center
	line-height 32px
	height 32px
	right 160px
	top 20px
	
.data-completeness-toggle
	border-right 1px solid $mainBorder
	display inline-block
	height 32px
	
.geocoder-container
	right 415px
	top 20px
	
.simpleGeocoder .esriGeocoder input
	box-shadow none
	height 20px
	
.tree-data-section
	margin 10px
	
.tree-data-section input[type='radio']
	margin 0 0 0 5px	
	
@import "ReactTree"
	
body:not(.mobile) .launch-map-button
	margin-right 10px
	float right